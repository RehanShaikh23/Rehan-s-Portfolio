    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.8.1",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.9.1",
